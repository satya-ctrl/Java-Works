//ARRAY OF OBJECTS
class B{
    int c,d;
    B(){c=2; d=5;}
    void display(){
        System.out.println("The value of c"+c);
        System.out.println("The value of d"+d);}
}
public class Area{
    public static void main(String[] args)
    
    {
        B[]a = new B[5];
        for(int i=0;i<5;i++)
        a[i].display();
    }
}
