import java.util.*;
public class Sum 
{
	public static void main(String args[]){
	Scanner sc = new Scanner(System.in);
	System.out.println("Enter five intergers");
	int[] arr = new int[5];
	for(int i =0;i<=5;i++){
	arr[i]=sc.nextInt();
	}
	int sum = 0;
	for(int i =0;i<=5;i=i+2){
	 sum=sum+arr[i];
	}
	System.out.println("the sum of alternate elements of arrayn is"+sum);
	}}
