class A{
    public static void  main (String args[]){
        int min=Integer.parseInt(args[0]);
        for (int i=0; i<3;i++)
        {
            int x=Integer.parseInt(args[i]);
            if(x<min)
            min=x;
        }
        
   
        System.out.print("min value is", min);
        
    }
}
